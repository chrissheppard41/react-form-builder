{
  "name": "react-form-builder-cs",
  "version": "0.5.0",
  "private": false,
  "dependencies": {
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.18",
    "copy-to-clipboard": "3.2.0",
    "react": "16.8.6",
    "react-dnd": "9.3.2",
    "react-dnd-html5-backend": "9.3.2",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "styled-components": "4.3.2",
    "typescript": "3.5.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@types/jest": "24.0.14",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "3.0.1",
    "jest": "24.7.1",
    "jooks": "1.0.0",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p ./src/tsconfig.json",
    "start": "react-scripts start",
    "build:old": "react-scripts build",
    "test": "react-scripts test --env=node",
    "test:single": "CI=true react-scripts test --env=node",
    "test:coverage": "npm test -- --coverage --env=node",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src/js/**/*.tsx --quiet --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT",
  "author": "Chris Sheppard",
  "main": "lib/build.js",
  "types": "index.d.ts",
  "husky": {
    "hooks": {
      "pre-push": "npm run test:single"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "js/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}